node {
stage ('SCM Checkout') {
   git 'https://github.com/javahometech/my-app'
}
stage ('Compile Package mvn') {
    sh 'mvn clean package'
}
stage ('email notification') {
 emailext body: '''Hi
 The build succeeded

 Thanks
 Jenkins Dev''', subject: 'Build success', to: 'snj.bahal@gmail.com'
}

}
